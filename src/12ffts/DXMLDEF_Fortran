        STRUCTURE /DXML_S_FFT_STRUCTURE/ 
           INTEGER*4 N
           LOGICAL*4 STRIDE_1_FLAG
           RECORD /DXML_C_FFT_STRUCTURE/ CV_STRUCT
           INTEGER*8 COS_RECOMB
