        INTEGER*4 ZCORR_NONPERIODIC_EXT,ZCORR_PERIODIC_EXT
        INTEGER*4 SCONV_NONPERIODIC_EXT,SCONV_PERIODIC_EXT
        INTEGER*4 SCORR_NONPERIODIC_EXT,SCORR_PERIODIC_EXT
