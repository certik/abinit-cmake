 mffmem 0  mkmem 0  natom  2 nband 8  nbdbuf 0
 nkpt  3
 nstep 20
 nsym 1
 ntypat  1
 occ 4*2.0d0 occopt  0  
 rprim   0 .5 .5  .5 0 .5  .5 .5 0
 tolwfr 1.0d-12
 typat  1 1 
 xred 3*0.00d0 3*0.25d0
 znucl 14
